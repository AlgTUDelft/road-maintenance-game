<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
	<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    
    <ui:with type="plangame.gwt.client.resource.locale.Lang" field="lang" />
    
    <g:VerticalPanel>
      <g:Label><ui:text from="{lang.DelayDialog_Delay}" /></g:Label>
      <g:ScrollPanel>
        <g:VerticalPanel ui:field="pnlTasks"></g:VerticalPanel>
      </g:ScrollPanel>
      <g:HorizontalPanel width="100%">
        <g:Button ui:field="btnDone"><ui:text from="{lang.Dialog_Done}" /></g:Button>
        <g:Button ui:field="btnDoneContinue"><ui:text from="{lang.DelayDialog_DoneContinue}" /></g:Button>
      </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>