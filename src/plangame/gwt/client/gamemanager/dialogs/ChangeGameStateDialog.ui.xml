<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
	<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:c="urn:import:plangame.gwt.client.widgets.controls">
    
    <ui:with type="plangame.gwt.client.resource.locale.Lang" field="lang" />
    
    <g:VerticalPanel>
      <g:Label><ui:text from="{lang.StateDialog_SelectState}" /></g:Label>
      <c:DataListBox ui:field="lstStates"></c:DataListBox>
      <g:HorizontalPanel width="100%">
        <g:Button ui:field="btnCancel"><ui:text from="{lang.Dialog_Cancel}" /></g:Button>
        <g:Button ui:field="btnOK"><ui:text from="{lang.Dialog_OK}" /></g:Button>
      </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>