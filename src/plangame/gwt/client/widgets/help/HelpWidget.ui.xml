<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	>
  
  <ui:with type="plangame.gwt.client.resource.ClientResource" field="res" />
  
  <g:HTMLPanel height="100%" width="100%">
    <g:HTML ui:field="divTitle" styleName="{res.helpcss.title}"></g:HTML>
    <g:HTML ui:field="divText" styleName="{res.helpcss.text}"></g:HTML>
  </g:HTMLPanel>
</ui:UiBinder> 