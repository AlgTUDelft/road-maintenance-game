<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" >

  <ui:with type="plangame.gwt.client.resource.ClientResource" field="res" />

	<g:FocusPanel ui:field="fcpTask" styleName="{res.plancss.task}">
		<g:HorizontalPanel>
			<g:AbsolutePanel ui:field="pnlTaskInfo" styleName="{res.plancss.taskinfo}">
				<g:Label ui:field="lblName"></g:Label>
			</g:AbsolutePanel>
			<g:AbsolutePanel ui:field="pnlTaskDelay" styleName="{res.plancss.taskdelay}">
				<g:Label ui:field="lblDelayProb"></g:Label>
			</g:AbsolutePanel>
		</g:HorizontalPanel>
	</g:FocusPanel>
</ui:UiBinder> 